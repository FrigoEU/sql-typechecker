{
  "main": "index.tsx",
  "dependencies": {
    "pgsql-ast-parser": "frigoeu/pgsql-ast-parser#plpgsql",
    "purify-ts": "^1.0.0",
    "typescript": "4.1.2"
  },
  "scripts": {
    "check": "tsc -p . --noEmit",
    "build": "node esbuild.js",
    "test": "npm run check && alsatian ./test/**/*.ts",
    "start": "npm run build && node --enable-source-maps ./out/main.js ",
    "debug": "npm run build && node --inspect-brk --enable-source-maps ./out/main.js ",
    "test_debug": "node --inspect-brk ./node_modules/alsatian/dist/cli/alsatian-cli.js ./test/**/*.ts -t 20000000"
  },
  "devDependencies": {
    "@types/node": "^14.14.10",
    "alsatian": "^3.2.1",
    "esbuild": "0.8.34",
    "prettier": "2.2.1"
  }
}
